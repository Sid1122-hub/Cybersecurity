crackle -i filename -o decrypt.pcap
aircrack-ng -b [BSSID] -w /usr/share/wordlists/rockyou.txt ptw.cap
aircrack-ng ptw.cap

tshark -r {pcap file} -Y "mqtt" -Tfields -mqtt.topic
tshark -r {pcap file} -Y "mqtt" -Tfields -mqtt.msg
